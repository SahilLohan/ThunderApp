  <body>
    
    <div class="top-container bg-dark">
      <% if(mainData.cod==200){ %>

      <h1> <center><%=City%></center> </h1>
      <ul>
        <li>Co-ordinates : <%=longitude%>,<%=latitude%></li>
        <li>Weather : <%=desc%></li>
        <li>Temperature : <%=temperature%></li>
        <li>Humidity : <%=humi%></li>
        <li>Pressure : <%=press%></li>
      </ul>
      <% }else{ %>
        <img src=<%= source%> alt=""> 
      <% } %>
    </div>

    <div class="bottom-container bg-dark " id="About">
